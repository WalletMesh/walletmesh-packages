{
  "name": "@walletmesh/aztec-example-wallet",
  "private": true,
  "version": "0.5.5",
  "type": "module",
  "publishConfig": {
    "publish": false
  },
  "scripts": {
    "dev": "echo \"Error: Due to web worker limitations, use 'pnpm build && pnpm preview' instead of 'pnpm dev'\" && exit 1",
    "build": "tsx scripts/build.ts",
    "build:force": "tsc --build && NODE_OPTIONS='--max-old-space-size=8192' vite build",
    "preview": "vite preview --port 5174",
    "clean": "rimraf dist node_modules",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "format": "biome format",
    "format:fix": "biome format --write",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@aztec/accounts": "catalog:",
    "@aztec/aztec.js": "catalog:",
    "@aztec/bb-prover": "catalog:",
    "@aztec/bb.js": "catalog:",
    "@aztec/foundation": "catalog:",
    "@aztec/noir-acvm_js": "catalog:",
    "@aztec/noir-contracts.js": "catalog:",
    "@aztec/noir-noirc_abi": "catalog:",
    "@aztec/noir-test-contracts.js": "catalog:",
    "@aztec/pxe": "catalog:",
    "@aztec/simulator": "catalog:",
    "@aztec/test-wallet": "catalog:",
    "@aztec/wallet-sdk": "catalog:",
    "@walletmesh/aztec-helpers": "workspace:^",
    "@walletmesh/aztec-rpc-wallet": "workspace:^",
    "@walletmesh/jsonrpc": "workspace:^",
    "@walletmesh/router": "workspace:^",
    "lodash-es": "^4.17.21",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "globals": "^16.3.0",
    "vite": "^7.1.4",
    "vite-plugin-bundlesize": "^0.3.0",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-plugin-static-copy": "^3.1.2",
    "vite-plugin-top-level-await": "^1.6.0"
  }
}
